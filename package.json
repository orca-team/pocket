{
  "private": true,
  "name": "orca-pocket",
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist -r git@github.com:NicoKam/orca-pocket-page.git",
    "docs:serve": "http-server docs-dist --spa",
    "build-all": "pnpm --filter=@orca-fe/* run build ",
    "build": "echo \"ok\"",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "c": "changeset",
    "cv": "changeset; changeset version;",
    "cv:pre": "changeset version --snapshot prepublish --snapshot-prerelease-template {tag}-${TIMESTAMP}",
    "pub": "npm run test && changeset publish && git push",
    "pub:only": "npm run test && changeset publish",
    "pub:pre": "changeset publish --tag prepublish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "lint-staged": "lint-staged --allow-empty",
    "test": "pnpm --filter=@orca-fe/* run test ",
    "test:coverage": "umi-test --coverage",
    "postinstall": "husky install"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@babel/runtime": "^7.23.6",
    "@changesets/cli": "^2.27.1",
    "@orca-fe/clsx-dedupe": "^1.2.1",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-router": "^5.1.20",
    "ahooks": "^3.7.8",
    "antd": "^5.12.5",
    "babel-plugin-import": "^1.13.8",
    "classnames": "^2.3.2",
    "dumi": "^2.2.16",
    "father": "^4.3.8",
    "file-saver": "^2.0.5",
    "gh-pages": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "zx": "^7.2.3"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@orca-fe/eslint-config": "^2.3.3",
    "@orca-fe/jss-plugin-auto-prefix": "^0.0.1",
    "@orca-fe/simple-jss": "^0.0.3",
    "@types/file-saver": "^2.0.7",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.19.3",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "globby": "^13.2.2",
    "inquirer": "^9.2.12",
    "jest": "^29.7.0",
    "jss": "^10.10.0",
    "react-json-view": "^1.21.3",
    "semver": "^7.5.4",
    "ts-node": "^10.9.2"
  }
}
